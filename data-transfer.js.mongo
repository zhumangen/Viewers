var cursor = db.targets.find({});
while (cursor.hasNext()) {
    let item = cursor.next();
    let needed = false;
    if (item.status === 1) {
    	var result = db.targets.find({studyInstanceUid: item.studyInstanceUid, seriesInstanceUid: item.seriesInstanceUid, status: 2});
    	if (!result.hasNext()) {
    		needed = true;
    	}
    } else if (item.status === 2) {
    	needed = true;
    }
    if (needed) {
    	item.location = map[item.location];
	    if (!!item.location) {
	    	item.status = 1;
	   		targetDb.targets.insert(item);
	    }
    }
}

